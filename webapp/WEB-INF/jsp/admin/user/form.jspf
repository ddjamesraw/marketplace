<%--@elvariable id="userForm" type="by.marketplace.web.form.UserForm" --%>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>
<%@page contentType="text/html; charset=UTF-8" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>${title}</title>
    </head>
    <body>
        <h1>${title}</h1>
        <form:form method="post" modelAttribute="userForm" 
        action="${pageContext.servletContext.contextPath}/admin/user/action/add">
             <form:label path="email">Email</form:label><br />
             <form:input path="email"/><br />
             <br />
             <form:label path="phone">Phone</form:label><br />
             <form:input path="phone"/><br />
             <br />
             <form:label path="city">City</form:label><br />
             <form:input path="city"/><br />
             <br />
             <input type="submit" value="Save" />
        </form:form>
    </body>
</html>