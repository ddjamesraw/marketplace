   <mapping class="by.marketplace.DAO.Entities.BrokerContractDAO"/>
   <mapping class="by.marketplace.DAO.Entities.BrokerServiceDAO"/>
   <mapping class="by.marketplace.DAO.Entities.BrokServicePlanDAO"/>
   <mapping class="by.marketplace.DAO.Entities.CurrencyDAO"/>
   <mapping class="by.marketplace.DAO.Entities.DepositoryDAO"/>
